{
  "name": "farmatup-api",
  "module": "index.ts",
  "type": "commonjs",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bunx tsc -p tsconfig.build.json",
    "postinstall": "npx prisma generate",
    "start": "npx prisma migrate deploy && node dist/index.js",
    "dev:node": "ts-node src/index.ts",
    "build:node": "tsc -p tsconfig.build.json",
    "start:node": "npx prisma migrate deploy && node dist/index.js",
    "postinstall:node": "npx prisma generate && npx prisma migrate deploy"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
